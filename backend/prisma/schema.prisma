generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
}

model Location {
  id        Int      @id @default(autoincrement())
  imei      String
  longitude Float
  height    Float
  latitude  Float
  timestamp DateTime @default(now())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@index([imei])
  @@index([timestamp])
}